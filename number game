import string
import random 
print("PASSWORD GENERATOR")
if _name_ == "_main_":
    s1=string.ascii_lowercase
    s2=string.ascii_uppercase
    s3=string.digits
    s4=string.punctuation
    plen=int(input("\nEnter The Length Of Your Password  - "))
    s=[]
    s.extend(list(s1))
    s.extend(list(s2))
    s.extend(list(s3))
    s.extend(list(s4))
    print("\nYour Password is: ")
    print("".join(random.sample(s,Â plen)))
